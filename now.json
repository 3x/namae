{
  "version": 2,
  "name": "namae",
  "alias": "namae.dev",
  "builds": [
    {
      "src": "/web/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "/api/services/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/availability/(?<provider>[^/]+)/(?<query>[^/]+)",
      "dest": "/api/services/$provider.js?query=$query"
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
