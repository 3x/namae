{
  "version": 2,
  "name": "namae",
  "alias": "namae.dev",
  "builds": [
    {
      "src": "/web/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    },
    {
      "src": "/api/services/*.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/availability/(?<provider>[^/]+)/(?<id>[^/]+)",
      "dest": "/api/services/$provider.js?name=$id",
      "headers": {
        "Cache-Control": "maxage=0, s-maxage=3600"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/web/$1"
    }
  ]
}
